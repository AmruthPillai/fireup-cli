{
  "name": "fireup-cli",
  "description": "Upload anything, right from your command line.",
  "version": "0.0.3",
  "author": "Amruth Pillai @AmruthPillai",
  "bin": {
    "fireup": "./bin/run"
  },
  "bugs": "https://github.com/AmruthPillai/fireup-cli/issues",
  "dependencies": {
    "@oclif/color": "^0.1.0",
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/plugin-help": "^2.2.3",
    "@oclif/plugin-not-found": "^1.2.3",
    "@oclif/plugin-update": "^1.3.9",
    "chalk": "^4.0.0",
    "clipboardy": "^2.3.0",
    "firebase-admin": "^8.10.0",
    "inquirer": "^7.1.0",
    "ora": "^4.0.3",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/inquirer": "^6.5.0",
    "@types/jsonfile": "^5.0.0",
    "@types/node": "^10.17.18",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^2.0.4",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=10.1.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/AmruthPillai/fireup-cli",
  "keywords": [
    "oclif",
    "firebase",
    "storage",
    "upload",
    "cli"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "fireup",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-update",
      "@oclif/plugin-not-found"
    ]
  },
  "repository": "AmruthPillai/fireup-cli",
  "scripts": {
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
